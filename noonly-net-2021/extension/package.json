{
  "name": "@noonly-net/extension",
  "private": true,
  "scripts": {
    "start": "react-scripts --openssl-legacy-provider start",
    "start:firefox": "cd dist/build/firefox && web-ext run",
    "build:sw": "tsc ./publicSrc/sw.ts --outDir ./dist/build/all",
    "build:content": "tsc ./publicSrc/content.ts --outDir ./dist/build/all",
    "build:css": "cpy ./publicSrc/chrome.css ./dist/build/chrome --rename=app.css && cpy ./publicSrc/firefox.css ./dist/build/firefox --rename=app.css",
    "build:firefox": "web-ext sign --source-dir=./dist/build/firefox --artifacts-dir=./dist/firefox --api-key='user:' --api-secret=''",
    "build_chrome": "node ./build_chrome.js",
    "build:chrome": "npm run build_chrome && move-cli --mkdirp true ./dist/build/chrome.crx ./dist/chrome/extension.crx && move-cli --mkdirp true ./dist/build/chrome.pem ./dist/chrome/key.pem",
    "build:public": "npm run build:sw && npm run build:content",
    "build:publicTwo": "npm run build:css && npm run build:manifest",
    "build:package": "npm run build:firefox && npm run build:chrome",
    "build:manifest": "cpy ./publicSrc/manifest_chrome.json ./dist/build/chrome --rename=manifest.json && cpy ./publicSrc/manifest_firefox.json ./dist/build/firefox --rename=manifest.json",
    "build:split": "ncp ./dist/build/all ./dist/build/chrome && move-cli ./dist/build/all ./dist/build/firefox",
    "build": "rimraf ./dist && react-scripts --openssl-legacy-provider build && npm run build:public && npm run build:split && npm run build:publicTwo",
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "aliases": "node create_aliases.js"
  },
  "dependencies": {
    "@bit/mui-org.material-ui.grid": "^4.9.10",
    "@chakra-ui/react": "^1.6.6",
    "@dnd-kit/core": "^3.1.1",
    "@emotion/react": "^11.4.0",
    "@emotion/styled": "^11.3.0",
    "@rematch/core": "^2.0.1",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^11.2.7",
    "@testing-library/user-event": "^12.8.3",
    "axios": "^0.21.1",
    "crypto-js": "^4.1.1",
    "dayjs": "^1.10.6",
    "framer-motion": "^4.1.17",
    "jwt-decode": "^3.1.2",
    "otplib": "^12.0.1",
    "react": "^17.0.2",
    "react-avatar-editor": "^12.0.0",
    "react-dnd-list": "^1.2.3",
    "react-dom": "^17.0.2",
    "react-dropzone": "^11.3.4",
    "react-helmet-async": "^1.0.9",
    "react-hook-form": "^7.15.0",
    "react-icons": "^4.2.0",
    "react-lazyload": "^3.2.0",
    "react-redux": "^7.2.4",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.3",
    "react-top-loading-bar": "^2.0.1",
    "redux": "^4.1.0",
    "rfdc": "^1.3.0",
    "streamsaver": "^2.0.5",
    "text-extensions": "^2.4.0",
    "web-vitals": "^1.1.2"
  },
  "devDependencies": {
    "@types/chrome": "0.0.155",
    "@types/crypto-js": "^4.0.2",
    "@types/react": "^17.0.20",
    "@types/react-avatar-editor": "^10.3.6",
    "@types/react-dom": "^17.0.9",
    "@types/react-router-dom": "^5.1.8",
    "@typescript-eslint/eslint-plugin": "^4.30.0",
    "@typescript-eslint/parser": "^4.30.0",
    "cpy-cli": "^3.1.1",
    "eslint": "^7.32.0",
    "move-cli": "^1.2.1",
    "ncp": "^2.0.0",
    "rimraf": "^3.0.2",
    "source-map-explorer": "^2.5.2",
    "typescript": "^4.4.2",
    "web-ext": "^6.4.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
