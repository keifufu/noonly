{
  "name": "aspire",
  "version": "0.0.1",
  "license": "MIT",
  "description": "Aspire",
  "author": {
    "name": "sirtalos34",
    "email": "sirtalos34@aspire.icu",
    "url": "https://aspire.icu"
  },
  "main": "public/electron.js",
  "private": true,
  "scripts": {
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "estart": "concurrently \"cross-env BROWSER=none npm run start\" \"wait-on http://localhost:3000 && electron .\"",
    "ebuild": "npm run build && electron-builder"
  },
  "build": {
    "productName": "Aspire",
    "appId": "icu.aspire.Aspire",
    "copyright": "Copyright Â© 2021 ${author}",
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    "dmg": {
      "contents": [
        {
          "x": 110,
          "y": 150
        },
        {
          "x": 240,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "linux": {
      "target": [
        "AppImage",
        "deb"
      ]
    },
    "win": {
      "target": "NSIS",
      "icon": "build/favicon.ico"
    }
  },
  "dependencies": {
    "@material-ui/core": "^4.11.2",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/lab": "^4.0.0-alpha.57",
    "@testing-library/jest-dom": "^5.11.6",
    "@testing-library/react": "^11.2.2",
    "@testing-library/user-event": "^12.5.0",
    "axios": "^0.21.1",
    "clipboard-copy": "^4.0.1",
    "clsx": "^1.1.1",
    "cross-env": "^7.0.3",
    "crypto-js": "^4.0.0",
    "dnd-core": "^11.1.3",
    "electron-is-dev": "^1.2.0",
    "fast-deep-equal": "^3.1.3",
    "file-saver": "^2.0.5",
    "formik": "^2.2.5",
    "formik-material-ui": "^3.0.1",
    "giframe": "^0.2.0",
    "howler": "^2.2.1",
    "invariant": "^2.2.4",
    "js-utl": "^4.37.0",
    "moment": "^2.29.1",
    "nan": "2.14.0",
    "notistack": "^1.0.2",
    "npm": "^6.14.11",
    "react": "^17.0.1",
    "react-dnd": "^11.1.3",
    "react-dnd-html5-backend": "^11.1.3",
    "react-dnd-preview": "^6.0.2",
    "react-dnd-scrolling": "^1.1.0",
    "react-dnd-touch-backend": "^11.1.3",
    "react-dom": "^17.0.1",
    "react-draggable": "^4.4.3",
    "react-infinite-scroller": "^1.2.4",
    "react-input-position": "^1.3.2",
    "react-js-utl": "^1.66.0",
    "react-lazyload": "^3.1.0",
    "react-markdown": "^5.0.3",
    "react-redux": "^7.2.2",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.1",
    "react-syntax-highlighter": "^15.4.3",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0",
    "remark-gfm": "^1.0.0",
    "rfdc": "^1.2.0",
    "simple-peer": "^9.9.3",
    "socket.io-client": "^3.0.5",
    "source-map-explorer": "^2.5.2",
    "text-extensions": "^2.3.0",
    "web-vitals": "^0.2.4",
    "zxcvbn": "^4.4.2"
  },
  "devDependencies": {
    "concurrently": "^5.3.0",
    "electron": "^11.2.1",
    "electron-builder": "^22.9.1",
    "electron-devtools-installer": "^3.1.1",
    "wait-on": "^5.2.1"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
