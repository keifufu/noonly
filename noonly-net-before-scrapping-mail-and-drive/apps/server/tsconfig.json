{
  "compilerOptions": {
    "module": "CommonJS",
    "sourceMap": true,
    "outDir": "dist",
    "baseUrl": "./",
    "paths": {
      "src/*": ["./src/*"],
      "prisma/*": ["./src/@generated/zod-prisma/*"]
    },
    "strict": true,
    "target": "ES6",
    "lib": ["ESNext", "DOM"],
    "noFallthroughCasesInSwitch": true,
    "forceConsistentCasingInFileNames": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "resolveJsonModule": true
  },
  "include": [
    "src"
  ]
}